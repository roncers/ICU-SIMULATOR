# Create a simulation environment
env <- simmer()
# Define the simulation process, e.g., arrival of customers
arrival <- trajectory() %>%
seize("server") %>%
timeout(function() rnorm(1, mean = 5)) %>%
release("server")
# Add the defined process to the environment
env %>%
add_resource("server", 1) %>%
add_generator("customer", arrival, function() rnorm(1, mean = 10))
# Run the simulation
sim_out <- env %>%
run(until = 100)  # Run the simulation for 100 units of time
View(arrival)
View(arrival_process)
View(env)
View(sim_out)
